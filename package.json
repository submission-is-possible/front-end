{
	"name": "front-end",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint . --ext .js,.ts,.svelte",
		"clean": "rimraf node_modules package-lock.json",
		"verify": "npm run clean && npm install && npm run lint && npm run check && npm run test && npm run build"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.0.2",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.8.5",
		"@vitest/browser": "^2.1.4",
		"@vitest/ui": "^2.1.4",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.14",
		"jest": "^29.7.0",
		"jsdom": "^25.0.1",
		"postcss": "^8.4.47",
		"rimraf": "^5.0.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.1.4",
		"eslint": "^7.32.0"
	}
}
